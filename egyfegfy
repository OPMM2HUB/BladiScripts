local OrionLib = loadstring(game:HttpGet(('https://raw.githubusercontent.com/shlexware/Orion/main/source')))()

local Window = OrionLib:MakeWindow({Name = "Bladi", HidePremium = false, SaveConfig = true, ConfigFolder = "OrionTest"})

local Tab = Window:MakeTab({
    Name = "Tab",
    Icon = "rbxassetid://4483345998",
    PremiumOnly = false
})

Tab:AddButton({
    Name = "Blazing Script",
    Callback = function()
              loadstring(game:HttpGet("https://api.luarmor.net/files/v3/loaders/7d354afc3102a5afa7a02c84536df20a.lua"))()
      end
})

Tab:AddButton({
    Name = "BladV2 Script (Takes times to load)",
    Callback = function()
          loadstring(game:HttpGet("https://paste.gg/p/anonymous/8416bf60ff3d4672a2e416f50e4d3a03/files/2a1dcd7c08074d7fb828e932538572c9/raw"))()
      end
})

Tab:AddButton({
    Name = "Silent Aim  Script (FAULTY)",
    Callback = function()
          loadstring(game:HttpGet("https://pastebin.com/raw/L0W9Bt1F"))()
      end
})

Tab:AddButton({
    Name = "YARHM Script",
    Callback = function()
          loadstring(game:HttpGet("https://raw.githubusercontent.com/Joystickplays/psychic-octo-invention/main/yarhm.lua", false))()
      end
})

Tab:AddButton({
    Name = "Highlight Hub Script (Key is hlvipcomingsoon)",
    Callback = function()
          loadstring(game:HttpGet("https://raw.githubusercontent.com/ThatSick/HighlightMM2/main/Main"))()
      end
})

Tab:AddButton({
    Name = "MM2 Autofarm Script",
    Callback = function()
          loadstring(game:HttpGet("https://raw.githubusercontent.com/ThatSick/HighlightMM2/main/Lite"))()
      end
})

OrionLib:Init()
